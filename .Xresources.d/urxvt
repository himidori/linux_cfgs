!! extensions
URxvt.perl-ext-common: default,clipboard,resize-font,url-select,keyboard-select,matcher
!! clipboard
URxvt.keysym.Shift-Control-C: perl:clipboard:copy
URxvt.keysym.Shift-Control-V: perl:clipboard:paste
URxvt.clipboard.copycmd: xsel -ib
!! url-select 
URxvt.keysym.Shift-Control-U: perl:url-select:select_next
URxvt.url-select.launcher:    firefox
URxvt.url-select.underline:   true
!! resize-font 
URxvt.resize-font.smaller: C-k
URxvt.resize-font.bigger: C-j
!! keyboard-select
URxvt.keysym.Mod-Escape:      perl:keyboard-select:activate
